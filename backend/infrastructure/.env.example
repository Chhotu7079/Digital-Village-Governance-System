# DVGS infrastructure env (copy to .env and adjust)

# Postgres
POSTGRES_USER=dvgs
POSTGRES_PASSWORD=dvgs
POSTGRES_PORT=5432

# Redis
REDIS_PORT=6379

# Kafka / Zookeeper
ZOOKEEPER_PORT=2181
KAFKA_PORT=9092
KAFKA_TOPIC_NOTIFICATION_EVENTS=notification-events

# RabbitMQ
RABBITMQ_PORT=5672
RABBITMQ_MGMT_PORT=15672
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# MinIO
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_BUCKET_ATTACHMENTS=dvgs-attachments
